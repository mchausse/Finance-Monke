{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [ false ],
        "no-console": false,
        "max-classes-per-file": [
            "error",
            { "ignoreExpressions": true, "max": 6 }
        ],
        "new-parens": ["error", "never"]
    },
    "rulesDirectory": []
}